<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:paddingBottom="0dp"
android:paddingLeft="0dp"
android:paddingRight="0dp"
android:paddingTop="0dp"
tools:context="com.example.alex.pruebaspinner.MainActivity"
android:background="#c188afd8">
<fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="306dp"
    android:layout_height="match_parent"
    tools:context="com.aexample.a41758511.myapplication.MapsActivity"
    android:layout_x="8dp"
    android:layout_y="11dp" />
    <View
        android:id="@+id/colored_bar"
        android:layout_width="304dp"
        android:layout_height="100dp"
        android:background="@color/blank"

        android:layout_x="09dp"
        android:layout_y="420dp"
        android:visibility="visible" />
    <Button
        android:id="@+id/btSubir"

        android:text="Subirme"
        android:textColor="#FFFFFF"
        android:textSize="27sp"

        android:layout_width="150dp"
        android:layout_height="46dp"
        android:background="@layout/buttonshape"
        android:shadowColor="#A8A8A8"
        android:shadowDx="1"
        android:shadowDy="4"
        android:shadowRadius="5"
        android:layout_x="170dp"
        android:layout_y="374dp"
        android:visibility="visible" />

    <TextView
        android:layout_width="295dp"
        android:layout_height="100dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/tvResult"
        android:layout_x="8dp"
        android:layout_y="420dp"
        android:textSize="16dp"
        android:visibility="visible" />
</AbsoluteLayout>